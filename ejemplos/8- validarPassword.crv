// Verifica que una contraseña tenga al menos 8 caracteres
mision validarPassword(password) {
  ejecutar:
    var longitud = calibre(password)
    var esValida = negativo
   
    estrategia
      si (longitud >= 8) {
        esValida = afirmativo
        reportar("Contraseña válida")
      }
      por defecto {
        reportar("Contraseña muy corta. Mínimo 8 caracteres")
      }
   
    retirada con esValida
}